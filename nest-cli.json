{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/dany-wallet/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/dany-wallet/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/dany-wallet",
  "projects": {
    "dany-wallet": {
      "type": "application",
      "root": "apps/dany-wallet",
      "entryFile": "main",
      "sourceRoot": "apps/dany-wallet/src",
      "compilerOptions": {
        "tsConfigPath": "apps/dany-wallet/tsconfig.app.json"
      }
    },
    "auth-micro": {
      "type": "application",
      "root": "apps/auth-micro",
      "entryFile": "main",
      "sourceRoot": "apps/auth-micro/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-micro/tsconfig.app.json"
      }
    },
    "transactions-micro": {
      "type": "application",
      "root": "apps/transactions-micro",
      "entryFile": "main",
      "sourceRoot": "apps/transactions-micro/src",
      "compilerOptions": {
        "tsConfigPath": "apps/transactions-micro/tsconfig.app.json"
      }
    }
  }
}